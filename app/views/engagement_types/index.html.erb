<h1>Engagement types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @engagement_types.each do |engagement_type| %>
      <tr>
        <td><%= engagement_type.name %></td>
        <td><%= link_to 'Show', engagement_type %></td>
        <td><%= link_to 'Edit', edit_engagement_type_path(engagement_type) %></td>
        <td><%= link_to 'Destroy', engagement_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

